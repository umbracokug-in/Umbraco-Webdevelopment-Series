@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.InnerPageModel>
@using ContentModels = DemoUmbraco.Models.CustomModels;

@{
    Layout = "Master.cshtml";

    var title = Model.Title;
    var description = Model.Description;


    <!-- Services-->
    <section class="page-section" id="services">
        <div class="container">
            <div class="text-center">
                @if (!string.IsNullOrWhiteSpace(title))
                {
                    <h2 class="section-heading text-uppercase">@title</h2>
                }
                @if (!string.IsNullOrWhiteSpace(description))
                {
                    <h3 class="section-subheading text-muted">@Html.Raw(description)</h3>
                }
            </div>
            
        </div>
    </section>
    <!-- Portfolio Grid-->


}
